{
  "name": "@graphql-debugger/monorepo",
  "version": "0.0.0-alpha.78",
  "private": true,
  "scripts": {
    "dev": "cross-env turbo dev --filter '!graphql-debugger' --filter '!@graphql-debugger/landing-page-app' --filter '!@graphql-debugger/landing-page-server' --concurrency=18",
    "build": "turbo build",
    "release": "cross-env DEBUG=\"@graphql-debugger:*\" node ./packages/utils/build/release.js",
    "test:sequential": "pnpm recursive exec -- sh -c 'pnpm run test || (echo \"Tests failed!\" && exit 1)'",
    "test": "turbo test",
    "clean": "find . -type d \\( -name \"node_modules\" -o -name \"build\" -o -name \".turbo\" \\) -exec rm -rf {} +",
    "lint": "eslint .",
    "format": "prettier --write .",
    "reset-db": "cd ./packages/data-access && npx prisma migrate reset --force"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.6",
    "@babel/preset-typescript": "7.23.3",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@tsconfig/node18": "18.2.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@types/supertest": "2.0.16",
    "@types/webpack": "5.28.5",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "autoprefixer": "10.4.16",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "42.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.4.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "jest": "29.7.0",
    "node-polyfill-webpack-plugin": "3.0.0",
    "postcss": "8.4.33",
    "postcss-loader": "7.3.3",
    "prettier": "3.1.1",
    "style-loader": "3.3.3",
    "supertest": "6.3.3",
    "svg-url-loader": "8.0.0",
    "tailwindcss": "3.4.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "turbo": "1.11.1",
    "typescript": "5.3.3",
    "url-loader": "4.1.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript",
        "ts-node",
        "@tsconfig/*",
        "@types/*",
        "jest",
        "@jest/*",
        "supertest",
        "prettier"
      ]
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
